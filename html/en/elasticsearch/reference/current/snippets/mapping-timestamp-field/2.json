GET my_index/_search
{
  "query": {
    "range": {
      "_timestamp": { 
        "gte": "2015-01-01"
      }
    }
  },
  "aggs": {
    "Timestamps": {
      "terms": {
        "field": "_timestamp", 
        "size": 10
      }
    }
  },
  "sort": [
    {
      "_timestamp": { 
        "order": "desc"
      }
    }
  ],
  "script_fields": {
    "Timestamp": {
      "script": "doc['_timestamp']" 
    }
  }
}
